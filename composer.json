{
  "name": "captenmasin/laravel-view-with",
  "description": "This is my package LaravelViewWith",
  "keywords": [
    "captenmasin",
    "laravel",
    "laravel-view-with"
  ],
  "homepage": "https://github.com/captenmasin/laravel-view-with",
  "license": "MIT",
  "authors": [
    {
      "name": "Mason Day",
      "email": "daymason@hotmail.co.uk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/contracts": "^8.0",
    "laravel/framework": "^8.36",
    "spatie/laravel-package-tools": "^1.4.3"
  },
  "require-dev": {
    "brianium/paratest": "^6.2",
    "guzzlehttp/guzzle": "^7.3",
    "nunomaduro/collision": "^5.3",
    "orchestra/testbench": "^6.15",
    "pestphp/pest": "^1.0",
    "phpunit/phpunit": "^9.3",
    "spatie/laravel-ray": "^1.9",
    "vimeo/psalm": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Captenmasin\\LaravelViewWith\\": "src",
      "Captenmasin\\LaravelViewWith\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Captenmasin\\LaravelViewWith\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Captenmasin\\LaravelViewWith\\LaravelViewWithServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
